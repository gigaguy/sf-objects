    <validationRules>
        <fullName>Conditional_status_approved</fullName>
        <active>false</active>
        <description>No reject reason should be selected when status is approved</description>
        <errorConditionFormula>ISPICKVAL( Status__c , &quot;Rejected&quot;) = False  &amp;&amp;  ISBLANK(TEXT( LOI_Reject_Reason__c )) = False</errorConditionFormula>
        <errorMessage>Reject reason should not be selected when status is other than Rejected.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Conditional_status_rejected</fullName>
        <active>true</active>
        <description>user can select reject reasons only when status is rejected</description>
        <errorConditionFormula>$Profile.Name = &quot;TQB 1st Line Supervisor Profile&quot;  &amp;&amp; ISPICKVAL( Status__c ,&quot;Rejected&quot;)   &amp;&amp;  ISBLANK(TEXT( LOI_Reject_Reason__c ))</errorConditionFormula>
        <errorMessage>LOI Reject Reason should be selected when status is in Rejected</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Status_field_value_1st_supervisor</fullName>
        <active>true</active>
        <description>1st line supervisor should select any status value other than Not Started, Submitted</description>
        <errorConditionFormula>$Profile.Name =&quot;TQB 1st Line Supervisor Profile&quot;  &amp;&amp; (ISPICKVAL( Status__c , &quot;Not Started&quot;) || ISPICKVAL( Status__c , &quot;In Progress&quot;))</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>Incorrect status value selected</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Status_value</fullName>
        <active>true</active>
        <description>Status field value should be valid one</description>
        <errorConditionFormula>$Profile.Name =&quot;TQB 1st Line Supervisor Profile&quot;  &amp;&amp;  ISBLANK(TEXT(  Status__c ))</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>Status field cannot be blank</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>condition_status_returned</fullName>
        <active>true</active>
        <description>if the user selects Returned as status value then review comments field should be filled out</description>
        <errorConditionFormula>$Profile.Name = &quot;TQB 1st Line Supervisor Profile&quot;  &amp;&amp; ISPICKVAL( Status__c ,&quot;Returned&quot;)  &amp;&amp;    LEN( Review_Comments__c ) = 0</errorConditionFormula>
        <errorMessage>Review comments should be filled out if the status is Returned</errorMessage>
    </validationRules>
</CustomObject>
